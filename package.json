{
  "name": "mialovebot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:frontail && npm run start:bot && pm2 logs",
    "start:bot": "pm2 start pm2.config.json --env production",
    "stop:bot": "pm2 delete pm2.config.json",
    "start:frontail": "frontail app.log -d --pid-path frontail.pid",
    "frontail": "frontail app.log",
    "logs": "pm2 logs",
    "dev": "node index.js",
    "dockerbuild": "docker build --tag jvoros/mialovebot-alpha .",
    "dockerrun": "docker run -d -p 80:9001 --name mialovebot-alpha jvoros/mialovebot-alpha"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^4.0.0",
    "frontail": "^4.1.0",
    "pm2": "^2.9.1",
    "twit": "^2.2.9"
  },
  "devDependencies": {
    "eslint": "^4.15.0"
  }
}
